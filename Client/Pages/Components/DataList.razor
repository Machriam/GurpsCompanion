<div class="d-flex justify-content-end col-md-12">
    <input class="form-control p-1" list="@_guid" @bind="SelectedText" />
    <button class="btn btn-danger p-1" @onclick="()=>SelectedText=default">&#10007</button>
</div>
<datalist id="@_guid">
    @foreach (var item in Items)
    {
        <option>@item.GetText()</option>
    }
</datalist>